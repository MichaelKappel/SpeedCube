{
  "compilerOptions": {
    "module": "System",
    "noImplicitAny": true,
    "noEmitOnError": false,
    "removeComments": true,
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "outDir": "wwwroot/Scripts",
    "rootDir": "./Scripts",
    "strict": true
  },
  "exclude": [
    "wwwroot"
  ]
}